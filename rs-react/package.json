{
  "name": "rs-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.tsx src/",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "dev": "ts-node server.ts",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:run-e2e": "cypress run --e2e",
    "cy:coverage": "npx nyc report --reporter=text-summary",
    "cypress": "start-server-and-test start http://localhost:3000/ cy:run-e2e && npm run cy:coverage"
  },
  "dependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.4",
    "express": "^4.18.2",
    "nyc": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "vite-plugin-istanbul": "^4.0.1",
    "vite-plugin-ssr": "^0.4.114"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-swc": "^3.3.0",
    "@vitest/coverage-c8": "^0.29.3",
    "c8": "^7.13.0",
    "cypress": "^12.10.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.59.2",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vitest": "^0.29.2"
  }
